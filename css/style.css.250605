#header.scroll > .contents_con {padding:0 min(20px, 1.04vw);}
#header.scroll > .contents_con > .logo_con {width:min(146px, 7.6vw); position:absolute; top:50%; left:min(20px, 1.04vw); z-index:10; transform:translateY(-50%);}
